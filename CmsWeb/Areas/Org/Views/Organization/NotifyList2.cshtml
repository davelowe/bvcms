@using CmsData
@model CmsData.Registration.Settings
<a class="notifylist" href="/Org/NotifyIds/@Model.OrgId" data-target="#usersDialog">
<div class="block border">
<b>choose users to notify (confirmation comes from first)</b>
@{  var q = from p in DbUtil.Db.PeopleFromPidString(Model.org.NotifyIds)
            select new { p.FromEmail, p.PeopleId };
    foreach (var p in q)
    {
    <div>@p.FromEmail &mdash; (@p.PeopleId)</div>
    }
}
</div>
</a>