@model CmsWeb.Areas.Search.Models.SavedQueryInfo
<div class="modal-header">
    <a class="close" data-dismiss="modal">x</a>
    <h4>Editing Saved Query</h4>
</div>
<div class="modal-body">
    <fieldset class="display">
        @Html.HiddenFor(m => m.QueryId)
        @Html.EditorFor(m => m.Name)
        @Html.EditorFor(m => m.Owner)
        @Html.EditorFor(m => m.Ispublic)
    </fieldset>
</div>
<div class="modal-footer">
    <a id="ExportToOld" class="btn btn-default" data-dismiss="modal">Export To Old SearchBuilder</a>
    <a class="btn btn-default" data-dismiss="modal">Cancel Edit</a>
    <a id="SaveQueryInfo" class="btn btn-default btn-primary">Save</a>
</div>