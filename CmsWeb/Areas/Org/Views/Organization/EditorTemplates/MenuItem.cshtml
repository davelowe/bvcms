@model CmsData.Registration.AskMenu.MenuItem
@using CmsWeb
@using (var ci = Html.BeginCollectionItem(Model.Name))
{
  <li>@Html.Raw(ci.hiddenindex)
    <ul>
      <li><div class="sgfee">Description:</div> @Html.TextBoxFor(m => m.Description, new { @class = "long" })
      <a class="del" href="#"><img align="top" src="/Content/images/delete.gif" title="remove this item" /></a></li>
      <li><div class="sgfee">SmallGroup:</div> @Html.TextBoxFor(m => m.SmallGroup) </li>
      <li><div class="sgfee">Fee:</div> @Html.TextBoxFor(m => m.Fee) </li>
    </ul>
  </li>
}
